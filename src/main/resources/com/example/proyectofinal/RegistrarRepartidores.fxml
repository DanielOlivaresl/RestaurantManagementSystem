<BorderPane fx:controller="com.example.proyectofinal.HelloController"
            xmlns:fx="http://javafx.com/fxml">

    <center>
        <VBox alignment="CENTER" spacing="10">
            <TextField fx:id="registrarRepartidoresTextField1" promptText="Nombre" maxWidth="150" onKeyReleased="#checkInputRegistroRepartidores"/>
            <TextField fx:id="registrarRepartidoresTextField2" promptText="Medio Transporte" maxWidth="150" onKeyReleased="#checkInputRegistroRepartidores"/>
            <TextField fx:id="registrarRepartidoresTextField3" promptText="Tiempo Entrega" maxWidth="150" onKeyReleased="#checkInputRegistroRepartidores" />
            <TextField fx:id="registrarRepartidoresTextField4" promptText="Efficiencia" maxWidth="150"  onKeyReleased="#checkInputRegistroRepartidores"/>
            <Button fx:id="registrarRepartidoresButton" text="Agregar Repartidor" onAction="#agregarRepartidores"/>
            <Label fx:id="registrarRepartidoresLabel"/>
        </VBox>
    </center>

    <left>
        <VBox alignment="BOTTOM_LEFT" style="-fx-padding: 10px;">
            <Button text="Regresar" onAction="#switchToScene1"/>
        </VBox>
    </left>



</BorderPane>

